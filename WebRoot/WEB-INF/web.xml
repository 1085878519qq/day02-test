<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">
  <servlet>
    <servlet-name>RequestDame</servlet-name>
    <servlet-class>jsp_07_servlect.RequestDame</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CustmoerList</servlet-name>
    <servlet-class>jsp_07_servlect.CustmoerList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>updServlet</servlet-name>
