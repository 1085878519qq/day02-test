  </servlet>
  <servlet>
    <servlet-name>UpdateServlet</servlet-name>
    <servlet-class>jsp_07_servlect.UpdateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyJsp</servlet-name>
